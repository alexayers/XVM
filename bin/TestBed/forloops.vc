/*
Example of for loop nesting.
*/

function int Main(int argc, array <string,*> argv)
{	
	for (int i = 0; i < 3; i++)
	{
        for (int j = 0; j < 3; j++)
		{
             for (int k = 0; k < 3; k++)
			 {
                  cout << "[ " << i << " " << j << " " << k << " ]" << endl;
			}
		}
	}
	
	return 0;
}

